# Production Environment Variables for Cloud Build
# Copy to .env.production and fill in real values
# These are passed as --build-arg to Docker during CI/CD

# API Backend URL (Cloud Run service URL)
VITE_API_URL=https://api.demeter.app

# Auth0 Configuration
VITE_AUTH0_DOMAIN=your-tenant.auth0.com
VITE_AUTH0_CLIENT_ID=your-client-id
VITE_AUTH0_AUDIENCE=https://api.demeter.app
VITE_AUTH0_CALLBACK_URL=https://your-industry.demeter.app/callback

# Disable auth bypass in production
VITE_AUTH_BYPASS=false

# Default tenant (optional, usually resolved from subdomain/JWT)
VITE_DEFAULT_TENANT_ID=
