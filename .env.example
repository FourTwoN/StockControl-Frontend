# API
VITE_API_URL=http://localhost:8080

# Auth0 (set real values for Full Setup, placeholders work with AUTH_BYPASS=true)
VITE_AUTH0_DOMAIN=demeter.auth0.com
VITE_AUTH0_CLIENT_ID=placeholder
VITE_AUTH0_AUDIENCE=https://api.demeter.app
VITE_AUTH0_CALLBACK_URL=http://localhost:3000/callback

# Local development bypass (set to true to skip Auth0 and use a fake dev user)
VITE_AUTH_BYPASS=true

# Tenant (used when AUTH_BYPASS=true; in production, resolved from subdomain or JWT)
VITE_DEFAULT_TENANT_ID=local
